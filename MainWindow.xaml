<Window x:Class="hackaton.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:hackaton"
        mc:Ignorable="d"
        
    Title="Mars invader" Height="672" Width="496" FocusManager.FocusedElement="{Binding ElementName=myCanvas}">
    <Canvas Name="myCanvas" Background="Black" Focusable="True" KeyDown="Canvas_KeyisDown"  >

        <Label Foreground="White" Name="enemies" FontSize="16" FontWeight="ExtraBold">Enemies:</Label>
        
    </Canvas>
</Window>
